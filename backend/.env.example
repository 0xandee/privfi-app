# Server Configuration
PORT=3001
NODE_ENV=development

# StarkNet Configuration
STARKNET_NETWORK=mainnet-alpha
STARKNET_RPC_URL=https://starknet-mainnet.infura.io/v3/YOUR_INFURA_KEY

# Proxy Wallet Configuration
PROXY_WALLET_ADDRESS=0x...
PROXY_WALLET_PRIVATE_KEY=0x...

# ETH Configuration for Gas
MIN_ETH_BALANCE=0.01
ALERT_ETH_THRESHOLD=0.0005

# AVNU API Configuration
AVNU_API_URL=https://starknet.api.avnu.fi
AVNU_INTEGRATOR_NAME=Privfi
AVNU_INTEGRATOR_FEE_BPS=15

# Typhoon Configuration
TYPHOON_CONTRACT_ADDRESS=0x...

# Security
CORS_ORIGIN=http://localhost:8080,http://localhost:8081

# Logging
LOG_LEVEL=info